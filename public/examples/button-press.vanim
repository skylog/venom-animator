        "alpha": [
          { "time": 0, "value": 0.3 },
          { "time": 200, "value": 0.4, "easing": "easeOutQuad" },
          { "time": 400, "value": 0.15, "easing": "easeOutQuad" },
          { "time": 800, "value": 0.3, "easing": "easeOutQuad" }
        ]
      }
    },
    {
