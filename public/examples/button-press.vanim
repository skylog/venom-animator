          { "time": 800, "value": 0, "easing": "easeOutQuad" }
        ]
      }
    },
    {
      "id": "btn-highlight",
      "type": "graphics",
      "blendMode": "add",
      "graphics": {
        "type": "roundRect",
        "x": 14,
        "y": 12,
        "width": 172,
        "height": 24,
        "radius": 10,
