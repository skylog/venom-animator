    { "id": "idle", "label": "Idle", "startTime": 0, "endTime": 100, "color": "#4a9eff" },
    { "id": "hover", "label": "Hover", "startTime": 100, "endTime": 200, "color": "#6bb3ff" },
    { "id": "press", "label": "Press", "startTime": 200, "endTime": 400, "color": "#2b7de9" },
    { "id": "release", "label": "Release", "startTime": 400, "endTime": 800, "color": "#4a9eff" }
